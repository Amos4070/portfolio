
{
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.8.10"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "python3.8"
    }
  },
  "installCommand": "apt-get update && apt-get install -y python3-dev gcc && pip install -r requirements.txt"
}